<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="LoopingViewPager">
        <attr name="isInfinite" format="boolean" />
        <attr name="autoScroll" format="boolean" />
        <attr name="scrollInterval" format="integer"/>
    </declare-styleable>

    <declare-styleable name="CustomShapePagerIndicator">
        <attr name="indicator_spacing" format="dimension"/>
        <attr name="indicator_size" format="dimension"/>
        <attr name="drawable_selected" format="reference"/>
        <attr name="drawable_normal" format="reference"/>
    </declare-styleable>

</resources>